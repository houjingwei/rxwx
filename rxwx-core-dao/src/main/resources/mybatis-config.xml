<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//ibatis.apache.org//DTD Config 3.0//EN"
        "http://ibatis.apache.org/dtd/ibatis-3-config.dtd">
<configuration>
	<objectFactory type="com.rxwx.core.page.PageObjectFactory" />
	<objectWrapperFactory type="com.rxwx.core.page.PageObjectWrapperFactory" />
	<plugins>
