<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.rxwx.admin.mapper.MenuMapper">


	<resultMap id="menuResultMap" type="com.rxwx.model.Menu">
		<id column="id" property="id" jdbcType="INTEGER" />
		<result column="name" property="name" jdbcType="VARCHAR" />
		<result column="createTime" property="createTime" jdbcType="DATE" />
		<result column="status" property="status" jdbcType="INTEGER" />
	</resultMap>

	<sql id="menuColumnList">
		id,name,createTime,status
	</sql>
	
	
	<select id="findAllMenu" resultMap="menuResultMap">
		select <include refid="menuColumnList" /> from menu
	</select>
	

</mapper>
