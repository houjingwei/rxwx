<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans        
    http://www.springframework.org/schema/beans/spring-beans.xsd        
    http://code.alibabatech.com/schema/dubbo        
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<dubbo:application name="${dubbo.application}" />
	<dubbo:registry address="${dubbo.registry}" />
	<dubbo:protocol id="dubbo1" name="dubbo" port="${dubbo.protocol}" />
	<dubbo:protocol id="dubbo2" name="dubbo" port="${dubbo.protocol2}" />
	
	
	<dubbo:reference  interface="com.rxwx.service.user.UserService" cluster="failover" retries="2" id="userService" check="false" timeout="10000"/>
	<dubbo:reference  interface="com.rxwx.service.cache.CacheService" cluster="failover" retries="2"  id="cacheService" check="false" timeout="10000"/>
	
</beans>